{
  "platform": "tiktok_shop",
  "module": "analytics",
  "endpoint_name": "shop_products_{product_id}_performance",
  "version": "202405",
  "summary": "Returns performance metrics for a product",
  "description": "Returns performance metrics for a product. This API currently provides data only for shops registered in the United States, United Kingdom, Singapore, Vietnam, and Thailand. Version\n202405\n  \nAPI Testing Tool",
  "path": "/analytics/202405/shop_products/{product_id}/performance",
  "http_method": [
    "GET"
  ],
  "parameters": {
    "path": [],
    "query": [
      {
        "properties": "shop_cipher Required",
        "type": "string",
        "description": "Use this property to pass shop information in requesting the API. Failure in passing the correct value when requesting the API for cross-border shops will return incorrect response.  Get by API Get Authorization Shop"
      },
      {
        "properties": "app_key Required",
        "type": "string",
        "description": "Every single app will have a unique key. Please use the specific key assigned to your app."
      },
      {
        "properties": "sign Required",
        "type": "string",
        "description": "Signature generated by gen algorithm. When you send API requests to TTS, you must sign them so that TTS can identify the senders."
      },
      {
        "properties": "timestamp Required",
        "type": "int",
        "description": "Unix timestamp GMT (UTC+00:00). This timestamp is used across all API requests. Developers can use this convert to local time."
      },
      {
        "properties": "start_date_ge Required",
        "type": "string",
        "description": "Start date (ISO 8601 YYYY-MM-DD format) in shop registered timezone. In the parameter name, \"ge\" refers to \"greater than or equal to\" (inclusive)"
      },
      {
        "properties": "end_date_lt Required",
        "type": "string",
        "description": "End date (ISO 8601 YYYY-MM-DD format) in shop registered timezone. In the parameter name, \"lt\" refers to \"less than\" (exclusive)"
      },
      {
        "properties": "with_comparison",
        "type": "bool",
        "description": "Whether previous period data is returned for comparison. Available values: true, false Default value: false The previous period has the same length and granularity as the current period with end time being the same as the start time of the current period. Example: If start_time_ge = 2024-04-01 and end_time_lt = 2024-04-08, the previous period data will be from 2024-03-25 to 2024-04-01."
      },
      {
        "properties": "granularity",
        "type": "string",
        "description": "Granularity of the data. Available values: ALL, 1D Default value: ALL * ALL: aggregate * 1D: daily"
      },
      {
        "properties": "currency",
        "type": "string",
        "description": "Currency. Available values: USD, LOCAL Default value: LOCAL * USD: US dollars * LOCAL: local currency where the shop is located"
      }
    ],
    "header": [
      {
        "properties": "content-type Required",
        "type": "string",
        "description": "Allowed type: application/json"
      },
      {
        "properties": "x-tts-access-token Required",
        "type": "string",
        "description": "The seller access_token value from Get Access Token, when user_type = 0. Follow this guide to get seller access_token."
      }
    ],
